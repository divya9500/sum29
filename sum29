#sum of multiplication
package Ass1;

import java.util.Scanner;

public class SumofNum {
    public static void main(String args[]){
        Scanner s=new Scanner(System.in);
        System.out.println("enter the number :");
        int n=s.nextInt();
        int sum=1;
        System.out.print("1");
        for(int i=1;i<=n;i++){
            sum=(sum*10)+1;
            System.out.print("+"+sum);
        }

    }
}
